{
    "dialogue_id": 12181,
    "interlocutors": [
        "FH",
        "BE"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "FH",
            "text": "こんにちは。",
            "timestamp": "2022-09-05T16:01:55.804000"
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "BE",
            "text": "こんにちは。",
            "timestamp": "2022-09-05T16:02:05.048000"
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "FH",
            "text": "暑いですね。",
            "timestamp": "2022-09-05T16:02:22.522000"
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "BE",
            "text": "ずっとエアコンつけています。",
            "timestamp": "2022-09-05T16:02:41.493000"
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "FH",
            "text": "さっき消してしまったので、暑くなっていて慌ててエアコンつけました。",
            "timestamp": "2022-09-05T16:02:58.466000"
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "BE",
            "text": "消すと途端に暑くなりますね。",
            "timestamp": "2022-09-05T16:03:25.210000"
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "FH",
            "text": "本当に、あっという間にびっくりする温度になっていました。",
            "timestamp": "2022-09-05T16:03:44.637000"
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "BE",
            "text": "室温がわかるようにしてますか？",
            "timestamp": "2022-09-05T16:04:03.417000"
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "FH",
            "text": "わかるようにはしていないのですが、汗が止まらなくなったので…きっと30度超えていました。",
            "timestamp": "2022-09-05T16:04:29.254000"
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "BE",
            "text": "室内で汗かくほどならだいぶ暑いですね。",
            "timestamp": "2022-09-05T16:04:52.030000"
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "FH",
            "text": "本当にだいぶ暑かったみたいで、しんどいです。",
            "timestamp": "2022-09-05T16:05:08.377000"
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "BE",
            "text": "熱中症にならない為にもエアコンは必須ですね。",
            "timestamp": "2022-09-05T16:05:29.478000"
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "FH",
            "text": "本当ですよね、エアコンの大事さを痛感しました。",
            "timestamp": "2022-09-05T16:05:42.222000"
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "BE",
            "text": "何度設定にしてますか？",
            "timestamp": "2022-09-05T16:06:21.016000"
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "FH",
            "text": "今は、２８度にしています。",
            "timestamp": "2022-09-05T16:06:33.966000"
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "BE",
            "text": "最近は28度設定でもたいぶ涼しく感じられますよね。",
            "timestamp": "2022-09-05T16:07:23.114000"
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "FH",
            "text": "そうですね。真夏だと、２６度とか２７度にしないと冷えなくて。",
            "timestamp": "2022-09-05T16:07:43.492000"
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "BE",
            "text": "9月になってようやく40度近い気温にならなくなりましたね。",
            "timestamp": "2022-09-05T16:08:16.601000"
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "FH",
            "text": "確かに。なんか、この夏はとんでもない気温だらけで感覚まひしました。",
            "timestamp": "2022-09-05T16:08:32.589000"
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "BE",
            "text": "毎年最高気温更新している気がします。",
            "timestamp": "2022-09-05T16:08:54.059000"
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "FH",
            "text": "猛暑、酷暑とかっていう日本語が追加されたとかでニュースになっていてラジオで茶化されていました。",
            "timestamp": "2022-09-05T16:09:26.666000"
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "BE",
            "text": "酷暑ってよくニュースで使われていますもんね、不思議な日本語。",
            "timestamp": "2022-09-05T16:10:07.229000"
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "FH",
            "text": "気象庁がわざわざ発表したとかなんとか…。",
            "timestamp": "2022-09-05T16:10:29.421000"
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "BE",
            "text": "そうなんですね！気象庁も大変だ。",
            "timestamp": "2022-09-05T16:10:51.212000"
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "FH",
            "text": "本当ですよね。でも、猛暑が３５度で酷暑が４０度だとかで…。",
            "timestamp": "2022-09-05T16:11:20.432000"
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "BE",
            "text": "ちゃんと基準があるんですね。40度越えが多いから言葉つくったんですかね。",
            "timestamp": "2022-09-05T16:12:03.432000"
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "FH",
            "text": "多分。あっ、日本気象協会が作ったらしいです。猛暑と酷暑だったら、字的には猛暑のほうがやばそう。",
            "timestamp": "2022-09-05T16:12:40.067000"
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "BE",
            "text": "酷暑は、30度越えとかでもひどく暑く感じますもんね。",
            "timestamp": "2022-09-05T16:13:24.830000"
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "FH",
            "text": "本当ですよね。命名するのが遅すぎたから感覚がよくわからない。",
            "timestamp": "2022-09-05T16:13:44.135000"
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "BE",
            "text": "最初からことば準備していてほしいですね。",
            "timestamp": "2022-09-05T16:13:58.724000"
        }
    ],
