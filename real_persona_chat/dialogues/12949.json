{
    "dialogue_id": 12949,
    "interlocutors": [
        "EU",
        "FR"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "EU",
            "text": "こんにちは！",
            "timestamp": "2022-09-14T17:29:36.487000"
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "FR",
            "text": "こんにちは",
            "timestamp": "2022-09-14T17:29:43.829000"
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "EU",
            "text": "ハマってることってあります？",
            "timestamp": "2022-09-14T17:30:14.278000"
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "FR",
            "text": "ゲームが好きなので、よくしますよ。",
            "timestamp": "2022-09-14T17:30:16.690000"
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "EU",
            "text": "スマホアプリのやつですか？",
            "timestamp": "2022-09-14T17:30:41.039000"
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "FR",
            "text": "いえ、パソコンでです。",
            "timestamp": "2022-09-14T17:30:44.509000"
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "EU",
            "text": "どんなゲームがあるのですか？",
            "timestamp": "2022-09-14T17:31:00.175000"
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "FR",
            "text": "シューティングゲームですね。敵と銃を撃ち合うみたいな。",
            "timestamp": "2022-09-14T17:31:17.854000"
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "EU",
            "text": "パソコンでゲームしたことないんですが、キーボードを使うってことですよね",
            "timestamp": "2022-09-14T17:31:50.221000"
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "FR",
            "text": "そうです。キーボードとマウスですね。",
            "timestamp": "2022-09-14T17:32:08.288000"
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "EU",
            "text": "マウス使うの難しそう！",
            "timestamp": "2022-09-14T17:32:15.557000"
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "FR",
            "text": "難しいですが、コントローラーよりはやりやすいと思います。",
            "timestamp": "2022-09-14T17:32:40.060000"
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "EU",
            "text": "そうなんです？私マウス使いが下手すぎて",
            "timestamp": "2022-09-14T17:33:13.365000"
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "FR",
            "text": "そうなんですね。普段のパソコン操作でも？",
            "timestamp": "2022-09-14T17:33:26.655000"
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "EU",
            "text": "はい、ビシッと定められない",
            "timestamp": "2022-09-14T17:33:49.201000"
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "FR",
            "text": "マウスパッドを使うといいですよ。",
            "timestamp": "2022-09-14T17:33:55.264000"
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "EU",
            "text": "マウスパッド使ってませんでした。やっぱり違いますか",
            "timestamp": "2022-09-14T17:34:26.303000"
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "FR",
            "text": "違うと思います。私もなかったらピシッとはできません。",
            "timestamp": "2022-09-14T17:34:37.213000"
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "EU",
            "text": "盲点でした。買ってみます",
            "timestamp": "2022-09-14T17:34:58.046000"
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "FR",
            "text": "あとは、マウスの操作感度を下げたりしてみても効果あると思います。",
            "timestamp": "2022-09-14T17:35:16.593000"
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "EU",
            "text": "それって、ＰＣから設定できるのでしょうか？",
            "timestamp": "2022-09-14T17:35:43.068000"
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "FR",
            "text": "できると思いますよ。調べたらやり方あると思います。",
            "timestamp": "2022-09-14T17:35:57.135000"
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "EU",
            "text": "ありがとうございます！これはこういう物だと思ってたから気にしなかった",
            "timestamp": "2022-09-14T17:36:33.437000"
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "FR",
            "text": "そうですね。ゲーマーにとっては重要なので、詳しくなりました。",
            "timestamp": "2022-09-14T17:36:55.198000"
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "EU",
            "text": "そのゲームはオンラインで他のプレーヤーと関わったりもありますか？",
            "timestamp": "2022-09-14T17:37:29.858000"
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "FR",
            "text": "そうです。基本的には人と戦います。",
            "timestamp": "2022-09-14T17:37:43.055000"
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "EU",
            "text": "それはめっちゃ面白そうです",
            "timestamp": "2022-09-14T17:38:10.985000"
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "FR",
            "text": "チームゲームなので、友達と話しながらやったり。",
            "timestamp": "2022-09-14T17:38:25.639000"
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "EU",
            "text": "そんなのもできるんですね！",
            "timestamp": "2022-09-14T17:38:38.360000"
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "FR",
            "text": "はい。楽しいですよ！",
            "timestamp": "2022-09-14T17:38:50.638000"
        }
    ],
