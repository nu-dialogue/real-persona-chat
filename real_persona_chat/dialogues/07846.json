{
    "dialogue_id": 7846,
    "interlocutors": [
        "BV",
        "AH"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "BV",
            "text": "こんにちはー！",
            "timestamp": "2022-08-03T20:00:03.943000"
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "AH",
            "text": "こんにちは！",
            "timestamp": "2022-08-03T20:00:07.416000"
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "BV",
            "text": "今日は何されていましたか？",
            "timestamp": "2022-08-03T20:00:21.439000"
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "AH",
            "text": "今日は一日中ゴロゴロしてました！",
            "timestamp": "2022-08-03T20:00:35.204000"
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "BV",
            "text": "いいですねー。暑くなかったですか？",
            "timestamp": "2022-08-03T20:00:48.552000"
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "AH",
            "text": "家の中はエアコンがあるので涼しかったですよ！",
            "timestamp": "2022-08-03T20:01:06.378000"
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "BV",
            "text": "家の中はいいですよね。私は外にいました。",
            "timestamp": "2022-08-03T20:01:23.553000"
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "AH",
            "text": "お仕事ですか？",
            "timestamp": "2022-08-03T20:01:34.215000"
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "BV",
            "text": "そうです。暑かったです。",
            "timestamp": "2022-08-03T20:01:49.043000"
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "AH",
            "text": "今日も最大気温高かったですもんね。",
            "timestamp": "2022-08-03T20:02:05.099000"
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "BV",
            "text": "最近特に暑いですよね。",
            "timestamp": "2022-08-03T20:02:15.430000"
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "AH",
            "text": "はい。昨日は35度超えていたので汗が止まりませんでした。",
            "timestamp": "2022-08-03T20:02:28.295000"
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "BV",
            "text": "これからどんどん気温上昇するんでしょうか。",
            "timestamp": "2022-08-03T20:02:47.100000"
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "AH",
            "text": "これ以上上がったら40度を超える場所も出てきそうです。",
            "timestamp": "2022-08-03T20:03:05.661000"
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "BV",
            "text": "倒れそうですね。",
            "timestamp": "2022-08-03T20:03:12.378000"
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "AH",
            "text": "はい、水分補給をしないといけないですね。",
            "timestamp": "2022-08-03T20:03:21.828000"
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "BV",
            "text": "塩分も大事ですね。",
            "timestamp": "2022-08-03T20:03:31.905000"
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "AH",
            "text": "水分だけでは熱中症対策にならないらしいですしね。",
            "timestamp": "2022-08-03T20:03:48.073000"
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "BV",
            "text": "汗で塩分も失われていますからね。",
            "timestamp": "2022-08-03T20:04:09.953000"
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "AH",
            "text": "経口補水液とかを飲むのが一番ですね。",
            "timestamp": "2022-08-03T20:04:29.291000"
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "BV",
            "text": "そうですね。体調は特にお変わりありませんか？",
            "timestamp": "2022-08-03T20:04:55.342000"
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "AH",
            "text": "私はいつも元気です！",
            "timestamp": "2022-08-03T20:05:04.643000"
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "BV",
            "text": "いいことですね。元気が一番です。",
            "timestamp": "2022-08-03T20:05:19.834000"
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "AH",
            "text": "<BV>さんは体調お変わりありませんか？",
            "timestamp": "2022-08-03T20:05:28.988000"
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "BV",
            "text": "私も元気ですよ。バッチリです！",
            "timestamp": "2022-08-03T20:05:49.151000"
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "AH",
            "text": "この調子で夏を乗り越えたいです。",
            "timestamp": "2022-08-03T20:06:00.305000"
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "BV",
            "text": "まだまだ暑さは続くので気をつけましょうね。",
            "timestamp": "2022-08-03T20:06:22.657000"
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "AH",
            "text": "あと２ヶ月くらいは気をつけないといけないですね。",
            "timestamp": "2022-08-03T20:06:39.609000"
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "BV",
            "text": "長いですね。",
            "timestamp": "2022-08-03T20:06:48.971000"
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "AH",
            "text": "長いですけど、耐えましょう！",
            "timestamp": "2022-08-03T20:07:02.404000"
        }
    ],
